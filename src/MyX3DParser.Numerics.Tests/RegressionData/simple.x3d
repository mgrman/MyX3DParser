  <Scene>
    <WorldInfo title='Prototype.x3d'/>
    <Transform>
      <Transform translation='0.0 0.6 0.0'>
        <!-- table top -->
        <Shape>
          <Appearance>
            <Material DEF='TableTopMaterial'>
            </Material>
          </Appearance>
          <Box size='1.2 0.2 1.2'/>
