      <Transform translation='-0.5 0.0 0.5'>
        <Shape USE='Leg'/>
        <!-- another table leg -->
      </Transform>
      <Transform translation='0.5 0.0 0.5'>
        <Shape USE='Leg'/>
        <!-- another table leg -->
      </Transform>
